{
    "name": "Amazon with LINKIT-II",
    "version": "0.2",
    "manifest_version": 2,

    "description": "KITLCにもあるかも！",

    "permissions": [
        "http://*/"
    ],

    "content_scripts": [
        {
            "matches": [
                "http://www.amazon.co.jp/*/dp/*",
                "http://www.amazon.co.jp/dp/*",
                "http://www.amazon.co.jp/gp/product/*"],
            "js": ["myscript.js"]
        }
    ]
}
